{
  "theme": "Catppuccin Mocha",
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "vim_mode": false,
  "ui_font_size": 14,
  "buffer_font_size": 12,
  "show_whitespaces": "all",
  "buffer_font_features": {
    "calt": false
  },
  "buffer_font_family": "RobotoMono Nerd Font",
  "relative_line_numbers": true,
  "assistant": {
    "enabled": false,
    "version": "2"
  },
  "features": {
    "inline_completion_provider": "none",
    "copilot": false
  },
  "file_types": {
    "Shell Script": ["zsh/.aliases", "zsh/.aliases.*"]
  }
}
